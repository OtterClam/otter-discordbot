{
  "name": "otter-discordbot",
  "version": "1.0.0",
  "description": "Discord bot for pulling cryptocurrency price data at intervals and displaying it in the users sidebar.",
  "scripts": {
    "start": "concurrently \"yarn:pricebot\" \"yarn:bondbot:mai\" \"yarn:bondbot:mai_clam\" \"yarn:rebasebot\"",
    "pricebot": "node pricebot.js",
    "bondbot:mai": "node bondbot.js MAI",
    "bondbot:mai_clam": "node bondbot.js MAI_CLAM",
    "rebasebot": "node rebasebot.js"
  },
  "author": "d0c0q@otterclam.finance",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^6.3.0",
    "discord.js": "^12.2.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.1"
  },
  "devDependencies": {
    "eslint": "^8.1.0"
  }
}
